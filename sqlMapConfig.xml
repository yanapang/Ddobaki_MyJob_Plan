<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<properties resource="com/example/demo/db/db.properties"/>
<typeAliases>
	<typeAlias type="com.example.demo.vo.DibsVO" alias="dibsVO"/>
	<typeAlias type="com.example.demo.vo.DogVO" alias="dogVO"/>
	<typeAlias type="com.example.demo.vo.PaymentVO" alias="paymentVO"/>
	<typeAlias type="com.example.demo.vo.ReservationVO" alias="reservationVO"/>
	<typeAlias type="com.example.demo.vo.UserInfoVO" alias="userInfoVO"/>
</typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/example/demo/db/DibsMapper.xml"/>
    <mapper resource="com/example/demo/db/DogMapper.xml"/>
    <mapper resource="com/example/demo/db/PaymentMapper.xml"/>
    <mapper resource="com/example/demo/db/ReservationMapper.xml"/>
    <mapper resource="com/example/demo/db/UserInfoMapper.xml"/>
  </mappers>
</configuration>