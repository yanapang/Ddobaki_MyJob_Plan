<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="place_image">
  <select id="findAll" resultType="place_imageVO">
    select * from place_image
  </select>
    <select id="findByNum" resultType="place_imageVO">
    select * from place_image where place_num = #{place_num}
  </select>
</mapper>
